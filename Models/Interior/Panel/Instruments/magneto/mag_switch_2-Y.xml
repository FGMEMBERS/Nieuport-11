<?xml version="1.0"?>

<!-- Mag Switch 1 -->
<PropertyList> 
	<path>mag_switch.ac</path>

	
<animation>
  <type>pick</type>
   <visible>false</visible>
  <object-name>on-mag</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/engines/engine/mag-switch-right</property>
    <step>1.0</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
    </binding>
  </action>
</animation>

<animation>
  <type>pick</type>
   <visible>false</visible>
  <object-name>off-mag</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/engines/engine/mag-switch-right</property>
    <step>-1.0</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
    </binding>
  </action>
</animation>

	<animation>
		<type>rotate</type>
		<object-name>switch</object-name>
		<property>controls/engines/engine/mag-switch-right</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-45</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>45</dep>
			</entry>
		</interpolation>
		<axis> 
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
		<center> 
			<x-m>0.0212</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0</z-m>
		</center>
	</animation>

</PropertyList>
